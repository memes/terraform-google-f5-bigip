{
  "$schema": "https://raw.githubusercontent.com/F5Networks/f5-declarative-onboarding/v1.12.0/src/schema/1.12.0/base.schema.json",
  "schemaVersion": "1.12.0",
  "class": "Device",
  "async": true,
  "label": "auto-onboard",
  "Common":{
    "class": "Tenant",
    "system": {
      "class": "System",
      "autoPhonehome": ${jsonencode(allow_phone_home)}
    },
    "dns": {
      "class": "DNS",
      "nameServers": [
        "169.254.169.254"
      ],
      "search": [
        "google.internal"
      ]
    },
    "ntp": {
      "class": "NTP",
      "servers": ${jsonencode(ntp_servers)},
      "timezone": "${timezone}"
    },
    "provision": {
      %{ for k,v in modules ~}
      "${k}": "${v}",
      %{ endfor ~}
      "class": "Provision"
    }
  }
}
